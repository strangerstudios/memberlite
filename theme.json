	"styles": {
		"blocks": {
			"core/avatar": {
				"css": "& img {border-radius: 999px}",
				"typography": {
					"lineHeight": "0"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--10)"
				}
			},
			"core/button": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--10)"
				},
				"variations": {
					"outline" : {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "1px"
						},
						"spacing": {
							"padding": {
								"top": "calc( var(--wp--preset--spacing--10) / 2)",
								"right": "var(--wp--preset--spacing--10)",
								"bottom": "calc( var(--wp--preset--spacing--10) / 2)",
								"left": "var(--wp--preset--spacing--10)"
							}
						}
					}
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "4.7rem"
				}
			},
			"core/image": {
				"variations": {
					"rounded" : {
						"border": {
						"radius": "var(--wp--custom--border--radius)"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						},
